<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tpm.entity.ContentTheo" table="理论课大纲课程内容">
		<id name="contentTheoid" column="课程内容ID">
			<generator class="native"></generator>
		</id>
		<property name="name" column="单元名称"></property>
		<property name="time" column="建议学时"></property>
		<property name="num" column="支撑教学目标"></property>
		<property name="aim" column="教学目标" length="5000"></property>
		<property name="content" column="本章主要内容" length="5000"></property>
		<!-- <property name="emphasis" column="本章重点" length="5000"></property> 
			<property name="nodus" column="本章难点" length="5000"></property> -->
		<property name="syllabusID" column="大纲理论课表ID"></property>


	</class>
</hibernate-mapping>
