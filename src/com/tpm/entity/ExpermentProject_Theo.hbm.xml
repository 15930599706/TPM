<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tpm.entity.ExpermentProject_Theo"
		table="理论课课内实验内容">
		<id name="expermentProject_Theoid" column="理论课实验内容ID">
			<generator class="native"></generator>
		</id>
		<property name="name" column="单元名称"></property>
		<property name="time" column="建议学时"></property>
		<property name="num" column="支撑教学目标"></property>
		<property name="equipment" column="实验设备及材料" length="5000"></property>
		<property name="theory" column="实验原理" length="5000"></property>
		<property name="request" column="教学要求" length="5000"></property>
		<property name="syllabusid" column="大纲课内实验表ID"></property>
	</class>
</hibernate-mapping>