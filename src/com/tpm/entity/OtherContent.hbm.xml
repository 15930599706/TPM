<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tpm.entity.OtherContent" table="理论课其他课程内容">
		<id name="otherContentid" column="项目ID">
			<generator class="native"></generator>
		</id>
		<property name="name" column="课程名称"></property>
		<property name="num" column="支撑教学目标"></property>
		<property name="aim" column="教学目标" length="2000"></property>
		<property name="content" column="教学内容" length="2000"></property>
		<property name="implementation" column="课程的实施" length="2000"></property>
		<property name="schedule" column="课程的进程安排" length="2000"></property>
		<property name="request" column="要求和成绩评定" length="2000"></property>
		<property name="syllabusID" column="大纲表ID"></property>
	</class>
</hibernate-mapping>
